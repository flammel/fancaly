{
    "name": "fancaly",
    "version": "2.0.0",
    "description": "A fancy calculator",
    "source": "src/index.html",
    "scripts": {
        "start": "parcel",
        "build": "parcel build",
        "lint": "eslint --ext .ts src && npm run format:check",
        "check": "tsc --noEmit",
        "check:watch": "tsc --noEmit --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "format": "prettier --write src/**/* .eslintrc.js .prettierrc.js jest.config.js jsconfig.json package.json tsconfig.json",
        "format:check": "prettier --check src/**/* .eslintrc.js .prettierrc.js jest.config.js jsconfig.json package.json tsconfig.json",
        "ci": "npm run lint && npm run check && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flammel/fancaly.git"
    },
    "author": "Florian Lammel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/flammel/fancaly/issues"
    },
    "homepage": "https://github.com/flammel/fancaly#readme",
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.7.0",
        "@parcel/service-worker": "^2.8.0",
        "@parcel/transformer-sass": "^2.7.0",
        "@parcel/transformer-webmanifest": "^2.7.0",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "buffer": "^5.5.0",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.1.2",
        "parcel": "^2.7.0",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@badrap/result": "^0.2.12",
        "@codemirror/autocomplete": "^6.4.0",
        "@codemirror/commands": "^6.1.2",
        "@codemirror/lint": "^6.1.0",
        "@codemirror/state": "^6.1.4",
        "@codemirror/view": "^6.7.1",
        "@types/bignumber.js": "^5.0.0",
        "bignumber.js": "^9.1.0",
        "js-base64": "^3.7.2"
    }
}
