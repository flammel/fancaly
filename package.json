{
    "name": "fancaly",
    "version": "2.0.0",
    "description": "A fancy calculator",
    "source": "src/index.html",
    "scripts": {
        "start": "parcel",
        "build": "rimraf dist && parcel build",
        "test": "jest",
        "test:watch": "jest --watch",
        "ts:check": "tsc --noEmit",
        "ts:watch": "tsc --noEmit --watch",
        "eslint:check": "eslint src",
        "eslint:fix": "eslint src",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "ci": "npm run prettier:check && npm run eslint:check && npm run ts:check && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flammel/fancaly.git"
    },
    "author": "Florian Lammel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/flammel/fancaly/issues"
    },
    "homepage": "https://github.com/flammel/fancaly#readme",
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@parcel/packager-raw-url": "^2.13.3",
        "@parcel/service-worker": "^2.13.3",
        "@parcel/transformer-sass": "^2.13.3",
        "@parcel/transformer-webmanifest": "^2.13.3",
        "buffer": "^6.0.3",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "parcel": "^2.13.3",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.2"
    },
    "dependencies": {
        "@badrap/result": "^0.2.13",
        "@codemirror/autocomplete": "^6.18.4",
        "@codemirror/commands": "^6.7.1",
        "@codemirror/lint": "^6.8.4",
        "@codemirror/state": "^6.5.0",
        "@codemirror/view": "^6.36.1",
        "bignumber.js": "^9.1.2",
        "js-base64": "^3.7.7"
    }
