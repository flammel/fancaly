{
  "name": "fancaly",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "prettier":
      "prettier --write .babelrc *.json src/*.ts src/*.js src/*.scss tests/*.ts",
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/*.ts' 'tests/*.ts'",
    "lintfix": "tslint --fix -c tslint.json 'src/*.ts' 'tests/*.ts'",
    "build": "parcel build src/index.html --public-url ./",
    "typecheck": "tsc --noEmit -p ./tsconfig.json"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2",
    "prettier": "^1.11.1",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "dependencies": {
    "bignumber.js": "^6.0.0"
  }
}
